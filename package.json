{
  "name": "sveltedraw",
  "description": "Pokas ideia, Svelte Ã© bom dms",
  "author": "Gabriel Capanema - @bielcapanema",
  "version": "0.0.3",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper export --legacy && mkdir public && mv -v ./__sapper__/export/* public/ ",
    "export": "sapper export --legacy",
    "start": "npx serve public",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "fix": "npx eslint --fix \"{,!(node_modules|public|__sapper__)/**/}*.{js,svelte}\"",
    "format": "npx prettier --write \"{,!(node_modules|public|__sapper__)/**/}*.{js,svelte}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all",
      "not safari < 12",
      "not kaios <= 2.5",
      "not edge < 79",
      "not chrome < 70",
      "not and_uc < 13",
      "not samsung < 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "compression": "^1.7.1",
    "open-color": "^1.7.0",
    "polka": "next",
    "roughjs": "^4.3.1",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.45.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.27.15",
    "svelte": "^3.23.2"
  }
}
